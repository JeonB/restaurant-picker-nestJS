{
  "compilerOptions": {
    "target": "ES5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "*": ["*"],
      "@_decorators/*": ["src/decorators/*"],
      "@_kakaoAPI/*": ["src/kakaoAPI/*"],
      "@_middlewares/*": ["src/middlewares/*"],
      "@_modules/*": ["src/modules/*"],
      "@_spec/*": ["src/spec/*"],
      "@_tests/*": ["src/tests/*"],
      "@_types/*": ["src/types/*"]
    }
  }
}
