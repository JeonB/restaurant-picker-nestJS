{
  "name": "back",
  "version": "1.0.0",
  "private": true,
  "description": "restaurant-picker back source",
  "scripts": {
    "build": "nest build",
    "start": "ts-node -r tsconfig-paths/register --files ./src/index.ts  | pino-pretty",
